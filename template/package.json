{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "0.0.1",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --mode=development --open --hot",
    {{#unitTest}}
    "test:unit": "mocha-webpack --webpack-config=webpack.config.coffee --require=spec/unit/setup.js spec/unit/**/*.spec.coffee",
    "test": "yarn run test:unit",
    {{/unitTest}}
    "build": "webpack --mode=production --progress --hide-modules",
    "lint:template": "pug-lint-vue src",
    "lint": "yarn run lint:template"
  },
  "dependencies": {
    "axios": "0.x.x",
    "vue": "2.x.x",
    "vue-router": "3.x.x"
  },
  "devDependencies": {
    {{#unitTest}}
    "@vue/test-utils": "latest",
    {{/unitTest}}
    {{#includes postCss "autoprefixer"}}
    "autoprefixer": "latest",
    {{/includes}}
    "babel-core": "latest",
    "babel-loader": "7.x.x",
    "babel-preset-env": "latest",
    "clean-webpack-plugin": "latest",
    "coffee-loader": "latest",
    "coffeescript": "latest",
    "css-loader": "latest",
    {{#unitTest}}
    "expect": "latest",
    {{/unitTest}}
    "file-loader": "latest",
    "friendly-errors-webpack-plugin": "latest",
    "html-webpack-plugin": "latest",
    {{#if_eq altCss "scss"}}
    "import-glob-loader": "latest",
    "jsdom": "latest",
    "jsdom-global": "latest",
    "mocha": "latest",
    "mocha-webpack": "2.0.0-beta.0",
    "node-sass": "latest",
    {{/if_eq}}
    "postcss": "6.x.x",
    {{#includes postCss "flexbugs-fixes"}}
    "postcss-flexbugs-fixes": "latest",
    {{/includes}}
    "postcss-loader": "latest",
    "pug": "latest",
    "pug-lint": "latest",
    "pug-lint-vue": "latest",
    "pug-loader": "latest",
    "pug-plain-loader": "latest",
    {{#if_eq altCss "scss"}}
    "sass-loader": "latest",
    "sass-resources-loader": "latest",
    {{/if_eq}}
    "style-loader": "latest",
    {{#if_eq altCss "stylus"}}
    "stylus": "latest",
    "stylus-loader": "latest",
    "stylus-resources-loader": "latest",
    {{/if_eq}}
    "tinyreset": "latest",
    "vue-loader": "15.x.x",
    "vue-pug-lint-loader": "latest",
    {{#unitTest}}
    "vue-server-renderer": "2.x.x",
    "vue-style-loader": "latest",
    {{/unitTest}}
    "vue-template-compiler": "latest",
    "webpack": "4.x.x",
    "webpack-cli": "2.x.x",
    "webpack-dev-server": "3.x.x",
    "webpack-merge": "latest"{{#unitTest}},{{/unitTest}}
    {{#unitTest}}
    "webpack-node-externals": "latest"
    {{/unitTest}}
  }
}
