{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "0.0.1",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    {{#if unitTest}}
    "test:unit": "cross-env NODE_ENV=test mocha-webpack --webpack-config webpack.config.js --require spec/unit/setup.js spec/unit/**/*.spec.coffee",
    "test": "yarn run test:unit",
    {{/if}}
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint:template": "pug-lint-vue src",
    "lint": "yarn run lint:template"
  },
  "dependencies": {
    "axios": "latest",
    "lodash": "latest",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-env": "latest",
    "clean-webpack-plugin": "latest",
    "coffee-loader": "latest",
    "coffeescript": "latest",
    "cross-env": "latest",
    "css-loader": "latest",
    {{#if unitTest}}
    "expect": "latest",
    "jsdom": "latest",
    "jsdom-global": "latest",
    "mocha": "latest",
    "mocha-webpack": "latest",
    {{/if}}
    "file-loader": "latest",
    "friendly-errors-webpack-plugin": "latest",
    "html-webpack-plugin": "latest",
    {{#if_eq altCss "scss"}}
    "import-glob-loader": "latest",
    "node-sass": "latest",
    {{/if_eq}}
    "postcss": "latest",
    "postcss-flexbugs-fixes": "latest",
    "postcss-loader": "latest",
    "pug": "latest",
    "pug-lint": "latest",
    "pug-lint-vue": "latest",
    "pug-loader": "latest",
    {{#if_eq altCss "scss"}}
    "sass-loader": "latest",
    "sass-resources-loader": "latest",
    {{/if_eq}}
    "style-loader": "latest",
    {{#if_eq altCss "stylus"}}
    "stylus": "latest",
    "stylus-loader": "latest",
    "stylus-resources-loader": "latest",
    {{/if_eq}}
    "tinyreset": "latest",
    "vue-loader": "latest",
    "vue-pug-lint-loader": "latest",
    "vue-template-compiler": "latest",
    {{#if unitTest}}
    "vue-test-utils": "latest",
    {{/if}}
    "webpack": "3.x.x",
    "webpack-cli": "2.x.x",
    "webpack-dev-server": "2.x.x",
    {{#if unitTest}}
    "webpack-merge": "latest",
    "webpack-node-externals": "latest"
    {{else}}
    "webpack-merge": "latest"
    {{/if}}
  }
}
